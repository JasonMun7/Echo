# Path to GCP service account JSON (for Firebase Admin + GCS)
GOOGLE_APPLICATION_CREDENTIALS=./service-account.json

# For npm run deploy (Cloud Run)
PROJECT_ID=your-gcp-project-id
REGION=us-central1

# When Firebase project != GCP project: Secret Manager secret name containing Firebase service account JSON
# FIREBASE_SA_SECRET=firebase-sa-key

# GCS bucket name for file storage
GCS_BUCKET=your-bucket-name

# Firebase project ID (optional, for reference)
FIREBASE_PROJECT_ID=your-firebase-project-id

# Gemini API key (required for workflow synthesis)
# Get from: https://aistudio.google.com/apikey
GEMINI_API_KEY=your-gemini-api-key
